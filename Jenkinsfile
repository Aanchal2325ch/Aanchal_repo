pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'Build SuccessFul'
        sleep 1
      }
    }

    stage('Test') {
      steps {
        echo 'Test successful'
      }
    }

    stage('Deploy') {
      steps {
        echo 'Deploy Successful'
      }
    }

    stage('End') {
      steps {
        echo 'Successfully executed'
      }
    }

  }
}